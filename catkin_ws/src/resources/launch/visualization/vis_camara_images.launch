<?xml version="1.0"?>
<launch>
  <!-- show images being published in camera/image_raw -->
  <!-- roslaunch p3at_tutorial vis_camera_images.launch -->
  <node name="image_view" pkg="image_view" type="image_view" respawn="false" output="screen">
    <remap from="image" to="/camera/image_raw" />
  </node>
</launch>
